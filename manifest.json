{
  "manifest_version": 2,

  "name": "AYINOPE PayPal Injector",
  "description": "This extension injects site cookies into paypal links",
  "version": "1.0",

  "browser_action": {
    "default_icon": "ayinope.png",
    "default_popup": "popup.html",
    "default_title": "PayPal Injector"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "cookies",
    "<all_urls>",
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.paypal.com/*","https://live.adyen.com/*"],
      "js": ["injector.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
